{
  "expo": {
    "name": "SlipStr",
    "slug": "SlipStr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
     "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sipstr",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show local offers and deliver to your area."
      }
    },
    "android": {
      "package": "com.sipstr",
      "permissions": ["ACCESS_FINE_LOCATION"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
